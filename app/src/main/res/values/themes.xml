<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Katrina" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorOnSecondary">#111318</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#111318</item>
    </style>

    <style name="Theme.Katrina" parent="Base.Theme.Katrina" />


    <style name="splashStyle" parent="Theme.AppCompat.NoActionBar">

        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>

        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="android:windowTranslucentNavigation">true</item>

        <item name="android:statusBarColor" >@color/new_black</item>
        <item name="colorSecondary">@color/new_black</item>
        <item name="colorSecondaryVariant">@color/new_black</item>
        <item name="colorOnSecondary">@color/new_black</item>

        <item name="android:windowBackground">@color/new_black</item>
    </style>

</resources>